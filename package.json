    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-loader-spinner": "^6.1.6",
    "react-resizable-panels": "^2.1.1",
    "react-router-dom": "^6.27.0",
    "styled-components": "^6.1.13",
    "typescript": "^4.4.2",
    "uuid": "^11.0.3",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "parcel",
    "build": "parcel build",
    "build-lib": "parcel build ./src/components/index.js --dist-dir=dist/lib",
    "release": "release-it",
    "release-beta": "release-it premajor --preReleaseId=beta --npm.tag=beta --github.preRelease",
    "release-alpha": "release-it --preRelease=alpha",
    "release-rc": "release-it --preRelease=rc",
    "release-ci": "release-it --ci",
    "release-debug": "release-it --debug --ci",
    "release-test": "release-it --dry-run",
    "watch": "parcel watch",
    "test": "jest",
    "format": "prettier --write .",
    "lint": "eslint src",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "fix-conflicts": "prettier --write . && eslint . --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "2.13.3",
    "@parcel/transformer-webmanifest": "2.13.3",
    "babel-jest": "^27.4.2",
    "babel-preset-react-app": "^10.0.1",
    "browserslist": ">= 4",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "conventional-recommended-bump": "^11.0.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "git-semver-tags": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.3",
    "jest-watch-typeahead": "^1.0.0",
    "parcel": "^2.13.3",
    "parcel-resolver-tspaths": "^0.0.9",
    "postcss": ">= 8",
    "postcss-normalize": "^13.0.1",
    "prettier": "^3.4.2",
    "process": "^0.11.10",
    "release-it": "^18.1.2",
    "semver": "^7.7.1"
  },
  "peerDependencies": {
    "@mui/icons-material": "^6.1.6",
    "@mui/material": "^6.1.6",
    "@mui/x-date-pickers": "^7.22.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0"
  },
  "targets": {
    "custom-interfafce-app": {
      "source": "public/index.html",
      "context": "browser"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "alias": {
    "$components": "./src/components",
    "$context": "./src/context/"
  }
}
